{
  "version": 2,
  "buildCommand": "cd apps/web && node scripts/vercel-build-setup.js && yarn build:vercel",
  "outputDirectory": "apps/web/build",
  "installCommand": "yarn install --ignore-engines --network-timeout 100000",
  "framework": "create-react-app",
  "env": {
    "SKIP_WORKERD": "true",
    "SKIP_PREFLIGHT_CHECK": "true",
    "DISABLE_ESLINT_PLUGIN": "true",
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}
